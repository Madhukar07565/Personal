<response id='paytm_payment_gateway_response'>
	<response-attributes>
		<response-attribute>
			<name>MID</name>
			<mapped-to>merchantID</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>ORDERID</name>
			<mapped-to>receiptNo</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>TXNAMOUNT</name>
			<mapped-to>amount</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>CURRENCY</name>
			<mapped-to>currency</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>TXNID</name>
			<mapped-to>transactionID</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>BANKTXNID</name>			
		</response-attribute>
		<response-attribute>
			<name>STATUS</name>
			<mapped-to>success</mapped-to>
			<validation-required-attributes>MID,ORDERID,TXNAMOUNT,CURRENCY,TXNID,BANKTXNID,STATUS,RESPCODE,RESPMSG,TXNDATE,GATEWAYNAME,BANKNAME,PAYMENTMODE</validation-required-attributes>
			<read-from-bean>paytmResponseStatusDecorator</read-from-bean>			
		</response-attribute>
		<response-attribute>
			<name>RESPCODE</name>
		</response-attribute>
		<response-attribute>
			<name>RESPMSG</name>
			<mapped-to>message</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>TXNDATE</name>
			<mapped-to>transactionDate</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>GATEWAYNAME</name>
		<mapped-to>gatewayId</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>BANKNAME</name>
		</response-attribute>
		<response-attribute>
			<name>PAYMENTMODE</name>
			<mapped-to>paymentMode</mapped-to>
		</response-attribute>
		<response-attribute>
			<name>CHECKSUMHASH</name>
		</response-attribute>
	</response-attributes>	
</response>	
