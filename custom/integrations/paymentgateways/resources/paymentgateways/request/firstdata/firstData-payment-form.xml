<form id='firstdata_payment_gateway_request'>
	<header-attributes>	
		<header-attribute> 
			<property>
			<name>method</name>
			<value>
				<default-value>paymentgateway.firstdata.action.method</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>	
			</property>
			<property>
			<name>name</name>
			<value>
				<default-value>paymentgateway.firstdata.form.name</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
			<property>
			<name>action</name>
			<value>
				<default-value>paymentgateway.firstdata.action.url</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
		</header-attribute>
	</header-attributes>

	<body-attributes>
		<body-attribute>
			<property>
				<name>x_login</name>
				<value>
					<default-value>paymentgateway.firstdata.login.id</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>x_fp_sequence</name>
				<value>
					<default-value></default-value>
					<read-from-bean>firstDataRandomSequenceNumberDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>x_fp_timestamp</name>
				<value>
					<default-value></default-value>
					<read-from-bean>firstDataTimeStampDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>x_amount</name>
				<value>
					<default-value>19.99</default-value>
					<mapped-to-user-selected-value>amount</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>x_currency_code</name>
				<value>
					<default-value>USD</default-value>
					<mapped-to-user-selected-value>currency</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>x_fp_hash</name>
				<value>
					<default-value>x_login,x_fp_sequence,x_fp_timestamp,x_amount</default-value>
					<read-from-bean>firstDataHashGeneratorDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>x_show_form</name>
				<value>
					<default-value>paymentgateway.firstdata.show.form.value</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>		
	</body-attributes>
</form>	
