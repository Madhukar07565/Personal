<form id='icici_payment_gateway_request'>
	<header-attributes>	
		<header-attribute> 
			<property>
			<name>method</name>
			<value>
				<default-value>paymentgateway.icici.action.method</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>	
			</property>
			<property>
			<name>name</name>
			<value>
				<default-value>paymentgateway.icici.form.name</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
			<property>
			<name>action</name>
			<value>
				<default-value>paymentgateway.icici.action.url</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
		</header-attribute>
	</header-attributes>

	<body-attributes>
		<body-attribute>
			<property>
				<name>txntype</name>
				<value>
					<default-value>paymentgateway.icici.txntype.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>timezone</name>
				<value>
					<default-value>paymentgateway.icici.timezone.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>txndatetime</name>
				<value>
					<default-value></default-value>
					<read-from-bean>iciciCurrentTimeDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>hash_algorithm</name>
				<value>
					<default-value>paymentgateway.icici.hash.algorithm.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>hash</name>
				<value>
					<default-value></default-value>
					<read-from-bean>icici_SHA256_HashDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>storename</name>
				<value>
					<default-value>paymentgateway.icici.storename.id</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>mode</name>
				<value>
					<default-value>paymentgateway.icici.mode.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>chargetotal</name>
				<value>
					<default-value>13.00</default-value>
					<mapped-to-user-selected-value>amount</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>currency</name>
				<value>
					<default-value>356</default-value>
					<mapped-to-user-selected-value>currency</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>		
		
		<body-attribute>
			<property>
				<name>customerid</name>
				<value>
					<default-value></default-value>
					<read-from-bean>defaultCustomerNameDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>language</name>
				<value>
					<default-value>paymentgateway.icici.language.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>full_bypass</name>
				<value>
					<default-value>paymentgateway.icici.fullbypass.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>responseSuccessURL</name>
				<value>
				<default-value>https://www.google.com</default-value>
				<!-- <default-value>/response/icici_payment_gateway_response</default-value>
					<read-from-bean>defaultSiteURLDecorator</read-from-bean> -->
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>responseFailURL</name>
				<value>
				<default-value>https://www.google.com</default-value>
				<!-- <default-value>/response/icici_payment_gateway_response</default-value>		
					<read-from-bean>defaultSiteURLDecorator</read-from-bean> -->		
				</value>	
			</property>
		</body-attribute>		
	</body-attributes>
</form>	
