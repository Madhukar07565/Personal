<form id='skash_cc_payment_gateway_request'>
	<header-attributes>	
		<header-attribute> 
			<property>
			<name>method</name>
			<value>
				<default-value>paymentgateway.skash.action.method</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>	
			</property>
			<property>
			<name>name</name>
			<value>
				<default-value>paymentgateway.skash.form.name</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
			<property>
			<name>action</name>
			<value>
				<default-value>paymentgateway.skash.action.url</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
		</header-attribute>
	</header-attributes>

	<body-attributes>
		<body-attribute>
			<property>
				<name>vleid</name>
				<value>
					<default-value>paymentgateway.skash.vle.id</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>ezeegotxnid</name>
				<value>
					<default-value></default-value>
					<mapped-to-user-selected-value>orderId</mapped-to-user-selected-value>
					<read-from-bean>defaultCustomerOrderIDDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>amount</name>
				<value>
					<default-value>10.0</default-value>
					<mapped-to-user-selected-value>amount</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>commission</name>
				<value>
					<default-value>paymentgateway.skash.vle.commission</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>		
		<body-attribute>
			<property>
				<name>ezzego_serviceid</name>
				<value>
					<default-value>paymentgateway.skash.service.id</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>service_provider_id</name>
				<value>
					<default-value>paymentgateway.skash.service.provider.id</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
	</body-attributes>
</form>	
