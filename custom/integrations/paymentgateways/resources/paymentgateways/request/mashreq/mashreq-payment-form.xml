<form id='mashreq_payment_gateway_request'>
	<header-attributes>	
		<header-attribute> 
			<property>
			<name>method</name>
			<value>
				<default-value>paymentgateway.mashreq.action.method</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>	
			</property>
			<property>
			<name>name</name>
			<value>
				<default-value>paymentgateway.mashreq.form.name</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
			<property>
			<name>action</name>
			<value>
				<default-value>paymentgateway.mashreq.action.url</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
		</header-attribute>
	</header-attributes>
	<body-attributes>
		<body-attribute>
			<property>
				<name>vpc_Version</name>
				<value>
					<default-value>paymentgateway.mashreq.vpc.version</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>vpc_Command</name>
				<value>
					<default-value>paymentgateway.mashreq.vpc.command</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>vpc_AccessCode</name>
				<value>
					<default-value>paymentgateway.mashreq.vpc.access.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>vpc_Merchant</name>
				<value>
					<default-value>paymentgateway.mashreq.vpc.merchant.id</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>vpc_MerchTxnRef</name>
				<value>
					<default-value></default-value>
					<read-from-bean>defaultCustomerOrderIDDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>vpc_OrderInfo</name>
				<value>
					<default-value></default-value>
					<mapped-to-user-selected-value>orderId</mapped-to-user-selected-value>
					<read-from-bean>defaultCustomerOrderIDDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>vpc_Amount</name>
				<value>
					<default-value>1000</default-value>
					<mapped-to-user-selected-value>amount</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>vpc_Currency</name>
				<value>
					<default-value>USD</default-value>
					<mapped-to-user-selected-value>currency</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>vpc_ReturnURL</name>
				<value>
					<default-value>/respose/mashreq</default-value>
					<read-from-bean>defaultSiteURLDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>vpc_Locale</name>
				<value>
					<default-value>paymentgateway.mashreq.vpc.locale.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>vpc_SecureHash</name>
				<value>
					<default-value>vpc_SecureHash,vpc_SecureHashType</default-value>
					<read-from-bean>mashreqSecureHashDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>vpc_SecureHashType</name>
				<value>
					<default-value>paymentgateway.mashreq.vpc.secure.hash.type</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>vpc_ReturnAuthResponseData</name>
				<value>
					<default-value>paymentgateway.mashreq.vpc.returnauthresponseData.option.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>					
	</body-attributes>
</form>	
