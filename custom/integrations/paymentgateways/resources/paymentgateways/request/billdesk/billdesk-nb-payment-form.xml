<form id='billdesk_nb_payment_gateway_request'>
	<header-attributes>	
		<header-attribute> 
			<property>
			<name>method</name>
			<value>
				<default-value>paymentgateway.billdesk.action.method</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>	
			</property>
			<property>
			<name>name</name>
			<value>
				<default-value>paymentgateway.billdesk.form.name</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
			<property>
			<name>action</name>
			<value>
				<default-value>paymentgateway.billdesk.action.url</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
		</header-attribute>
	</header-attributes>

	<body-attributes>
		<body-attribute>
			<property>
				<name>MerchantID</name>
				<value>
					<default-value>paymentgateway.billdesk.merchant.id</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>CustomerID</name>
				<value>
					<default-value></default-value>
					<read-from-bean>defaultCustomerNameDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>	
		<body-attribute>
			<property>
				<name>TxnAmount</name>
				<value>
					<default-value>2.00</default-value>
					<mapped-to-user-selected-value>amount</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>CurrencyType</name>
				<value>
					<default-value>INR</default-value>
					<mapped-to-user-selected-value>currency</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>TypeField1</name>
				<value>
					<default-value>paymentgateway.billdesk.type.field1</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>SecurityID</name>
				<value>
					<default-value>paymentgateway.billdesk.security.id</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>TypeField2</name>
				<value>
					<default-value>paymentgateway.billdesk.type.field2</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>RU</name>
				<value>
					<default-value>/response/billdesk_nb</default-value>
					<read-from-bean>defaultSiteURLDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>msg</name>
				<value>
					<default-value>MerchantID,CustomerID,TxnAmount,CurrencyType,TypeField1,SecurityID,TypeField2,RU</default-value>
					<read-from-bean>billDeskCheckSumMsgDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>					
	</body-attributes>
</form>	
