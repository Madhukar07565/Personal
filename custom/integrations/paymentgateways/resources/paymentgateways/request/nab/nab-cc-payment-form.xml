<form id='nab_cc_payment_gateway_request'>
	<header-attributes>	
		<header-attribute> 
			<property>
			<name>method</name>
			<value>
				<default-value>paymentgateway.nab.action.method</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>	
			</property>
			<property>
			<name>name</name>
			<value>
				<default-value>paymentgateway.nab.form.name</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
			<property>
			<name>action</name>
			<value>
				<default-value>paymentgateway.nab.action.url</default-value>
				<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
			</value>
			</property>	
		</header-attribute>
	</header-attributes>

	<body-attributes>
		<body-attribute>
			<property>
				<name>EPS_MERCHANT</name>
				<value>
					<default-value>paymentgateway.nab.eps.merchant</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>TransactionPassword</name>
				<value>
					<default-value>paymentgateway.nab.transaction.password</default-value>
					<add-to-submitform>false</add-to-submitform>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_TXNTYPE</name>
				<value>
					<default-value>paymentgateway.nab.eps.transaction.type.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_REFERENCEID</name>
				<value>
					<default-value></default-value>
					<mapped-to-user-selected-value>orderId</mapped-to-user-selected-value>			
					<read-from-bean>defaultCustomerOrderIDDecorator</read-from-bean>
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_AMOUNT</name>
				<value>
					<default-value>1.00</default-value>
					<mapped-to-user-selected-value>amount</mapped-to-user-selected-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_TIMESTAMP</name>
				<value>
					<default-value>YYYYMMddHHmmss</default-value>
					<read-from-bean>nabGMTCurrentTimeDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>		
		<body-attribute>
			<property>
				<name>EPS_PAYMENTCHOICE</name>
				<value>
					<default-value>paymentgateway.nab.eps.payment.choice.code</default-value>
					<read-from-bean>defaultPropertiesDecorator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_RESULTURL</name>
				<value>
				<!-- <default-value>http://www.google.com</default-value> -->
					<default-value>/response/nab_cc</default-value>					
					<read-from-bean>defaultSiteURLDecorator</read-from-bean>
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_CARDNUMBER</name>
				<value>
					<default-value></default-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_EXPIRYMONTH</name>
				<value>
					<default-value></default-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_EXPIRYYEAR</name>
				<value>
					<default-value></default-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_CCV</name>
				<value>
					<default-value></default-value>
					<read-from-bean></read-from-bean>	
				</value>	
			</property>
		</body-attribute>
		<body-attribute>
			<property>
				<name>EPS_FINGERPRINT</name>
				<value>
					<default-value>EPS_MERCHANT,TransactionPassword,EPS_TXNTYPE,EPS_REFERENCEID,EPS_AMOUNT,EPS_TIMESTAMP</default-value>
					<read-from-bean>nabFingerPrintGeneratorDecocator</read-from-bean>	
				</value>	
			</property>
		</body-attribute>
	</body-attributes>
</form>	
